        <Param val="# parametters&amp;#10;&amp;#10;fixation_duration = 0.5&amp;#10;&amp;#10;rep_condition = 1 # nb rep per blocchi sperimentali&amp;#10;&amp;#10;&amp;#10;&amp;#10;# nb rep blocchi sperimentali&amp;#10;num = (int(expInfo['participant']) % 2)&amp;#10;&amp;#10;if  num == 0:&amp;#10;    nb_rep_ita = 1*rep_condition&amp;#10;    nb_rep_eng = 0 &amp;#10;    blocco_order = &quot;ITA_ENG&quot;&amp;#10;else:&amp;#10;    nb_rep_eng = 1*rep_condition   &amp;#10;    nb_rep_ita = 0&amp;#10;    blocco_order = &quot;ENG_ITA&quot;&amp;#10;" valType="extendedCode" updates="constant" name="Begin Experiment"/>
        <Param val="fixation_duration = 0.5;&amp;#10;rep_condition = 1;&amp;#10;num = (Number.parseInt(expInfo[&quot;participant&quot;]) % 2);&amp;#10;if ((num === 0)) {&amp;#10;    nb_rep_ita = (1 * rep_condition);&amp;#10;    nb_rep_eng = 0;&amp;#10;    blocco_order = &quot;ITA_ENG&quot;;&amp;#10;} else {&amp;#10;    nb_rep_eng = (1 * rep_condition);&amp;#10;    nb_rep_ita = 0;&amp;#10;    blocco_order = &quot;ENG_ITA&quot;;&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
