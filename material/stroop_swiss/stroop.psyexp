        <Param val="if (practice_kb.corr) {&amp;#10;    msg = `Bravo! RT=${practice_kb.rt}`;&amp;#10;} else {&amp;#10;    msg = &quot;Oops! non \u00e8 coretto&quot;;&amp;#10;}&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
